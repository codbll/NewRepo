                                            <dt style="background-color:#FFF;width:95%;"><span style="color:red;font-weight:bold;margin-left:10px;">YURT DIŞI ÜNİVERSİTE BAŞARI ORANI :</span></dt>
                                            <dd style="width:95%;background-color:#FFF;"><span style="font-weight:bold;margin-left:10px;"><a href="@Model.Kurumlar.YurtDisiUniversiteBasariOrani" target="_blank">@Model.Kurumlar.YurtDisiUniversiteBasariOrani</a></span></dd>
                                        }
                                        @if (Model.Kurumlar.BursOlanakları != null)
                                        {
                                            <dt style="background-color:#FFF;width:95%;"><span style="color:red;font-weight:bold;margin-left:10px;">BURS OLANAKLARI :</span></dt>
                                            <dd style="width:95%;background-color:#FFF;"><span style="font-weight:bold;margin-left:10px;"><a href="@Model.Kurumlar.BursOlanakları" target="_blank">@Model.Kurumlar.BursOlanakları</a></span></dd>
                                        }
                                        @if (Model.Kurumlar.Ucreti != null)
                                        {
                                            <dt style="background-color:#FFF;width:95%;"><span style="color:red;font-weight:bold;margin-left:10px;">ÜCRETİ :</span></dt>
                                            <dd style="width:95%;background-color:#FFF;"><span style="font-weight:bold;margin-left:10px;"><a href="@Model.Kurumlar.Ucreti" target="_blank">@Model.Kurumlar.Ucreti</a></span></dd>
                                        }


                                    </dl>
                                </div>
                            </div>
                        }
                        
                    </div>


                </div>


            </div>

        </div>

        <div class="col-md-12" style="margin-left:-28px;">
            <ul class="image-gallery sort-destination full-width">
                @foreach (var item in ViewData["detayReklam"] as List<Mobit.Data.Context.Slider>)
                {
                    <li class="isotope-item">
                        <div class="image-gallery-item" style="padding:5px;">
                            <a href="@item.Url" target="@item.Target">
                                <span class="thumb-info thumb-info-centered-info">
                                    <span class="thumb-info-wrapper">
                                        <img src="~/Upload/slide/@item.Resim" class="img-responsive img-thumbnail" alt="@item.Resim" style="height:300px;">
                                    </span>
                                </span>
                            </a>
                        </div>
                    </li>
                }
            </ul>
        </div>

    </div>

</div>
