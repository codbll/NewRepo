                                                    {
                                                        foreach (var resim in Model.Kurumlar.KurumResim.Where(r => r.ResimTipi == 4))
                                                        {
                                                            <div class="image-gallery-item">
                                                                <span class="thumb-info thumb-info-lighten">
                                                                    <span class="thumb-info-wrapper">
                                                                        @if (@Model.Kurumlar.Sponsorlar != null)
                                                                        {
                                                                            <img src="~/Upload/slide/@resim.Resim" class="img-responsive" alt="@Model.Kurumlar.KurumAdi" title="@Model.Kurumlar.KurumAdi" style="height:150px;">
                                                                        }
                                                                        else
                                                                        {
                                                                            <img src="~/Upload/kurum/@Model.Kurumlar.UploadYolu/@resim.Resim" class="img-responsive" alt="@Model.Kurumlar.KurumAdi" title="@Model.Kurumlar.KurumAdi" style="height:150px;">
                                                                        }
                                                                        <span class="thumb-info-title">
                                                                            <span class="thumb-info-inner" style="font-size:15px;">@resim.Aciklama</span>
                                                                        </span>
                                                                    </span>
                                                                </span>
                                                            </div>
                                                        }
                                                    }
                                                </div>
                                            </div>
                                        </div>
                                        <div class="tab-pane" id="aciklama">
                                            <p>
                                                @Html.Raw(Model.Kurumlar.Aciklama)
                                            </p>
                                        </div>
                                        <div class="tab-pane" id="video">
                                            @if (Model.Kurumlar.Video != null && Model.Kurumlar.Video.Contains("iframe"))
                                            {
                                                @Html.Raw(Model.Kurumlar.Video)
                                            }
                                            else
                                            {
                                                <iframe src="@Html.Raw(Model.Kurumlar.Video)" width="100%" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>
                                            }
                                        </div>
                                        <div class="tab-pane" id="harita">
                                            @if (Model.Kurumlar.Harita != null && Model.Kurumlar.Harita.Contains("iframe"))
                                            {
                                                @Html.Raw(Model.Kurumlar.Harita)
                                            }
                                            else
                                            {
                                                <iframe src="@Html.Raw(Model.Kurumlar.Harita)" width="100%" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>
                                            }
                                        </div>
                                        <div class="tab-pane" id="3d">
                                            @if (Model.Kurumlar.D3Boyutlu != null && Model.Kurumlar.D3Boyutlu.Contains("iframe"))
                                            {
                                                @Html.Raw(Model.Kurumlar.D3Boyutlu)
                                            }
                                            else
                                            {
                                                <iframe src="@Html.Raw(Model.Kurumlar.D3Boyutlu)" width="100%" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>
                                            }
                                        </div>
                                        <div class="tab-pane" id="panoroma">
                                            @if (Model.Kurumlar.Panoroma != null && Model.Kurumlar.Panoroma.Contains("iframe"))
                                            {
                                                @Html.Raw(Model.Kurumlar.Panoroma)
                                            }
                                            else
                                            {
                                                <iframe src="@Html.Raw(Model.Kurumlar.Panoroma)" width="100%" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>
                                            }
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

