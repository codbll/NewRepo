﻿@model Mobit.Models.KurumDetayModel
@using Mobit.Helpers
@{

    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.bilgi = "";
}


@section baslikAlan{

    <div class="row">
        <div class="col-md-12">
            <ul class="breadcrumb">
                <li> @{ if (@Model.Kurumlar.Kategoriler != null) { @Model.Kurumlar.Kategoriler.KategoriAdi} }</li>
                <li>@Model.Kurumlar.KurumAdi</li>
            </ul>
        </div>
    </div>
}

<div role="main" class="main shop" style="padding-left:2px;padding-right:25px;margin-left:7px;">
    <div class="container">
        <div class="row" style="padding-left:2px;padding-right:25px;">
            <div class="col-md-12 img-thumbnail" style="padding:7px;margin-top:-10px;margin-left:7px;margin-right:20px;">
                <div class="col-md-2">
                    @if (@Model.Kurumlar.Sponsorlar != null)
                    {
                        <img class="img-responsive img-thumbnail" src="~/Upload/slide/@Model.Kurumlar.Resim" style="height:150px;" alt="@Model.Kurumlar.KurumAdi">
                    }
                    else
                    {
                        <img class="img-responsive img-thumbnail" src="~/Upload/kurum/@Model.Kurumlar.UploadYolu/@Model.Kurumlar.Resim" style="height:150px;" alt="@Model.Kurumlar.KurumAdi">
                    }
                </div>
                <div class="col-md-10">
                    <br />
                    <span style="font-size:60px;vertical-align:bottom;text-align: center;" class="heading-primary">@Model.Kurumlar.KurumAdi</span>
                </div>
            </div>
        </div>
    </div>

    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="row">
                    <div class="col-md-9">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="tabs tabs-tertiary tabs-center nav-tabs-justified">
                                    <ul class="nav nav-tabs">
