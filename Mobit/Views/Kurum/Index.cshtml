                                                    @if (Model.Kurumlar.KurumResim.Count() > 0)
                                                    {
                                                        foreach (var resim in Model.Kurumlar.KurumResim.Where(r => r.ResimTipi == 1))
                                                        {
                                                            <div class="image-gallery-item">
                                                                <span class="thumb-info thumb-info-lighten">
                                                                    <span class="thumb-info-wrapper">
                                                                        @if (@Model.Kurumlar.Sponsorlar != null)
                                                                        {
                                                                            <img src="~/Upload/slide/@resim.Resim" class="img-responsive" alt="@Model.Kurumlar.KurumAdi" title="@Model.Kurumlar.KurumAdi" style="width:100%;height:400px;">

                                                                        }
                                                                        else
                                                                        {
                                                                            <img src="~/Upload/kurum/@Model.Kurumlar.UploadYolu/@resim.Resim" class="img-responsive" alt="@Model.Kurumlar.KurumAdi" title="@Model.Kurumlar.KurumAdi" style="width:100%;height:400px;">
                                                                        }
                                                                        
                                                                        <span class="thumb-info-title">
                                                                            <span class="thumb-info-inner">@resim.Aciklama</span>
                                                                            <span class="thumb-info-type">@resim.Kurumlar.KurumAdi</span>
                                                                        </span>
                                                                    </span>
                                                                </span>
                                                            </div>
                                                        }
                                                    }
                                                </div>
                                                <div class="owl-carousel owl-theme manual thumb-gallery-thumbs mt" id="thumbGalleryThumbs">

                                                    @if (Model.Kurumlar.Resim.Count() > 0)
                                                    {
                                                        foreach (var resim in Model.Kurumlar.KurumResim.Where(r => r.ResimTipi == 1))
                                                        {
                                                            <div class="image-gallery-item">
                                                                <span class="thumb-info thumb-info-lighten">
                                                                    <span class="thumb-info-wrapper">
                                                                        @if (@Model.Kurumlar.Sponsorlar != null)
                                                                        {
                                                                            <img src="~/Upload/slide/@resim.Resim" class="img-responsive" alt="@Model.Kurumlar.KurumAdi" title="@Model.Kurumlar.KurumAdi" style="height:150px;">
                                                                        }
                                                                        else
                                                                        {
                                                                            <img src="~/Upload/kurum/@Model.Kurumlar.UploadYolu/@resim.Resim" class="img-responsive" alt="@Model.Kurumlar.KurumAdi" title="@Model.Kurumlar.KurumAdi" style="height:150px;">
                                                                        }
                                                                        <span class="thumb-info-title">
                                                                            <span class="thumb-info-inner" style="font-size:15px;">@resim.Aciklama</span>
                                                                        </span>
                                                                    </span>
                                                                </span>
                                                            </div>
                                                        }
                                                    }
                                                </div>
                                            </div>
                                        </div>
                                        <div class="tab-pane" id="aktiviteler">
                                            <div class="thumb-gallery">
                                                <div class="owl-carousel owl-theme manual thumb-gallery-detail show-nav-hover" id="thumbGalleryDetail3">
                                                    @if (Model.Kurumlar.KurumResim.Count() > 0)
                                                    {
                                                        foreach (var resim in Model.Kurumlar.KurumResim.Where(r => r.ResimTipi == 4))
                                                        {
                                                            <div class="image-gallery-item">
                                                                <span class="thumb-info thumb-info-lighten">
                                                                    <span class="thumb-info-wrapper">
                                                                        
                                                                        @if (@Model.Kurumlar.Sponsorlar != null)
                                                                        {
                                                                            <img src="~/Upload/slide/@resim.Resim" class="img-responsive" alt="@Model.Kurumlar.KurumAdi" title="@Model.Kurumlar.KurumAdi" style="width:100%;height:400px;">
                                                                        }
                                                                        else
                                                                        {
