        <div class="modal-content">
            <div class="modal-header">
                <div class="col-md-11"><h4 class="modal-title" id="largeModalLabel" style="font-size:18px;font-weight:bold;color:#000000;">HABERLER</h4></div>
                <div class="col-md-1"><button type="button" class="btn btn-default" data-dismiss="modal">KAPAT</button></div>     
            </div>
            <div class="modal-body">
                @Html.Partial("/Views/_Partial/_HaberPopup.cshtml", ViewData["HaberlerPopup"])
            </div>
        </div>
    </div>
</div>

