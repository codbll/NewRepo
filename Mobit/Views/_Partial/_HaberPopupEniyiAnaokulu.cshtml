

    <br />
    @{
        Mobit.Data.Context.Entities db = new Mobit.Data.Context.Entities();
        foreach (var Model in db.Kurumlar.Where(h => h.Sponsorlar == "Ayın En İyi Anaokulu").Take(1).ToList())
        {
            <div class="row" style="padding-left:2px;padding-right:25px;">
                <div class="col-md-12 img-thumbnail">
                    <div class="col-md-2">
                        <img class="img-responsive img-thumbnail" src="~/Upload/slide/@Model.Resim" style="height:100px;" alt="@Model.KurumAdi">
                    </div>
                    <div class="col-md-10">
                        <br />
                        <span style="font-size:50px;vertical-align:bottom;text-align: center;" class="heading-primary">@Model.KurumAdi</span>
                    </div>
                </div>
            </div>
            <br />
            <div class="panel-group" id="accordionSuccess">
                <div class="panel panel-accordion panel-accordion-success">
                    <div class="panel-heading" style="background-color:aliceblue;color:blue;font-weight:bold;">
                        <h4  style="background-color:aliceblue;color:blue;font-weight:bold;">
                            <a  class="accordion-toggle" style="background-color:aliceblue;color:blue;font-weight:bold;"  data-toggle="collapse" data-parent="#accordionSuccess" href="#collapseSuccessHaberler">
                                Haberler
                            </a>
                        </h4>
                    </div>
                    <div id="collapseSuccessHaberler" class="accordion-body collapse">
                        <div class="panel-body">
                            @foreach (var item in db.Haberler.Where(h => h.Aktif == true && h.KategoriId == 13).OrderByDescending(u => u.Id).ToList())
                            {
                                <div class="lightbox">
                                    <a href="~/Upload/slide/@item.Resim">
                                        <span class="thumb-info thumb-info-lighten">
                                            <span class="thumb-info-wrapper">
                                                <img alt="@item.Ad" title="@item.Ad" class="img-responsive img-thumbnail" src="~/Upload/slide/@item.Resim" style="width:100%;height:400px;">
                                            </span>
                                        </span>
                                    </a>
                                </div>
                            }

                        </div>
                    </div>
                </div>
                @if (Model.KurumResim.Where(r => r.ResimTipi == 1).Count() >= 1)
                {
                    <div class="panel panel-accordion panel-accordion-success">
                        <div class="panel-heading" style="background-color:aliceblue;color:blue;font-weight:bold;">
                            <h4  style="background-color:aliceblue;color:blue;font-weight:bold;">
                                <a  class="accordion-toggle" style="background-color:aliceblue;color:blue;font-weight:bold;"  data-toggle="collapse" data-parent="#accordionSuccess" href="#collapseSuccessslayt" >
                                    Tanıtım Slaytları
                                </a>
                            </h4>
                        </div>
                        <div id="collapseSuccessslayt" class="accordion-body collapse">
                            <div class="panel-body">
                                <div class="owl-carousel owl-theme show-nav-hover" data-plugin-options='{"items": 1, "margin": 10, "loop": true, "nav": true, "dots": true,"autoplay": true, "autoplayTimeout": 1000,"autoplayHoverPause": true}'>
                                    @foreach (var item in Model.KurumResim.Where(r => r.ResimTipi == 1))
                                    {
                                        <div class="lightbox">
                                            <a href="~/Upload/slide/@item.Resim">
                                                <span class="thumb-info thumb-info-lighten">
                                                    <span class="thumb-info-wrapper">
                                                        <img alt="@item.Aciklama" title="@item.Aciklama" class="img-responsive img-thumbnail" src="~/Upload/slide/@item.Resim" style="width:100%;height:400px;">
                                                    </span>
                                                </span>
                                            </a>
                                        </div>
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                }

                @if (Model.KurumResim.Where(r => r.ResimTipi == 4).Count() >= 1)
                {
                    <div class="panel panel-accordion panel-accordion-success">
                        <div class="panel-heading" style="background-color:aliceblue;color:blue;font-weight:bold;">
                            <h4  style="background-color:aliceblue;color:blue;font-weight:bold;">
                                <a  class="accordion-toggle" style="background-color:aliceblue;color:blue;font-weight:bold;"  data-toggle="collapse" data-parent="#accordionSuccess" href="#collapseSuccessAktiviteler">
                                    Aktiviteler
                                </a>
                            </h4>
                        </div>
                        <div id="collapseSuccessAktiviteler" class="accordion-body collapse">
                            <div class="panel-body">
                                @foreach (var item in Model.KurumResim.Where(r => r.ResimTipi == 4))
                                {
                                    <div class="lightbox">
                                        <a href="~/Upload/slide/@item.Resim">
                                            <span class="thumb-info thumb-info-lighten">
                                                <span class="thumb-info-wrapper">
                                                    <img alt="@item.Aciklama" title="@item.Aciklama" class="img-responsive img-thumbnail" src="~/Upload/slide/@item.Resim" style="width:100%;height:400px;">
                                                </span>
                                            </span>
                                        </a>
                                    </div>
                                }

                            </div>
                        </div>
                    </div>
                }
                @if (Model.Aciklama != null && Model.Aciklama.Length > 10)
                {
                    <div class="panel panel-accordion panel-accordion-success">
                        <div class="panel-heading" style="background-color:aliceblue;color:blue;font-weight:bold;">
                            <h4  style="background-color:aliceblue;color:blue;font-weight:bold;">
                                <a  class="accordion-toggle" style="background-color:aliceblue;color:blue;font-weight:bold;"  data-toggle="collapse" data-parent="#accordionSuccess" href="#collapseSuccessaciklama">
                                    Hakkında
                                </a>
                            </h4>
                        </div>
                        <div id="collapseSuccessaciklama" class="accordion-body collapse">
                            <div class="panel-body">
                                <p>
                                    @Html.Raw(Model.Aciklama)
                                </p>
                            </div>
                        </div>
                    </div>
                }
                @if (Model.Video != null && Model.Video.Length > 20)
                {
                    <div class="panel panel-accordion panel-accordion-success">
                        <div class="panel-heading" style="background-color:aliceblue;color:blue;font-weight:bold;">
                            <h4  style="background-color:aliceblue;color:blue;font-weight:bold;">
                                <a  class="accordion-toggle" style="background-color:aliceblue;color:blue;font-weight:bold;"  data-toggle="collapse" data-parent="#accordionSuccess" href="#collapseSuccessvideo">
                                    Tanıtım Videosu
                                </a>
                            </h4>
                        </div>
                        <div id="collapseSuccessvideo" class="accordion-body collapse">
                            <div class="panel-body">
                                @Html.Raw(Model.Video)
                            </div>
                        </div>
                    </div>
                }
                @if (Model.Harita != null && Model.Harita.Length > 20)
                {
                    <div class="panel panel-accordion panel-accordion-success">
                        <div class="panel-heading" style="background-color:aliceblue;color:blue;font-weight:bold;">
                            <h4  style="background-color:aliceblue;color:blue;font-weight:bold;">
                                <a  class="accordion-toggle" style="background-color:aliceblue;color:blue;font-weight:bold;"  data-toggle="collapse" data-parent="#accordionSuccess" href="#collapseSuccessHarita">
                                    Harita
                                </a>
                            </h4>
                        </div>
                        <div id="collapseSuccessHarita" class="accordion-body collapse">
                            <div class="panel-body">
                                @Html.Raw(Model.Harita)
                            </div>
                        </div>
                    </div>

                }
                @if (Model.Panoroma != null && Model.Panoroma.Length > 10)
                {
                    <div class="panel panel-accordion panel-accordion-success">
                        <div class="panel-heading" style="background-color:aliceblue;color:blue;font-weight:bold;">
                            <h4  style="background-color:aliceblue;color:blue;font-weight:bold;">
                                <a  class="accordion-toggle" style="background-color:aliceblue;color:blue;font-weight:bold;"  data-toggle="collapse" data-parent="#accordionSuccess" href="#collapseSuccessPanoroma">
                                    Panoroma
                                </a>
                            </h4>
                        </div>
                        <div id="collapseSuccessPanoroma" class="accordion-body collapse">
                            <div class="panel-body">
                                @Html.Raw(Model.Panoroma)


                                @*<iframe src="@Html.Raw(Model.Panoroma)" width="100%" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>*@
                            </div>
                        </div>
                    </div>
                }
                @if (Model.D3Boyutlu != null && Model.D3Boyutlu.Length > 10)
                {
                    <div class="panel panel-accordion panel-accordion-success">
                        <div class="panel-heading" style="background-color:aliceblue;color:blue;font-weight:bold;">
                            <h4  style="background-color:aliceblue;color:blue;font-weight:bold;">
                                <a  class="accordion-toggle" style="background-color:aliceblue;color:blue;font-weight:bold;"  data-toggle="collapse" data-parent="#accordionSuccess" href="#collapseSuccess3d">
                                    3D Tanıtım
                                </a>
                            </h4>
                        </div>
                        <div id="collapseSuccess3d" class="accordion-body collapse">
                            <div class="panel-body">
                                @*<iframe src='@Html.Raw(Model.D3Boyutlu)' width='100%' height='450' frameborder='0' style='border:0' allowfullscreen></iframe>*@
                                @Html.Raw(Model.D3Boyutlu)
                            </div>
                        </div>
                    </div>
                }
                <div class="panel panel-accordion panel-accordion-success">
                    <div class="panel-heading" style="background-color:aliceblue;color:blue;font-weight:bold;">
                        <h4  style="background-color:aliceblue;color:blue;font-weight:bold;">
                            <a  class="accordion-toggle" style="background-color:aliceblue;color:blue;font-weight:bold;"  data-toggle="collapse" data-parent="#accordionSuccess" href="#collapseSuccessResimler">
                                Resimler
                            </a>
                        </h4>
                    </div>
                    <div id="collapseSuccessResimler" class="accordion-body collapse">
                        <div class="panel-body">
                            @foreach (var item in Model.KurumEgitimciler.Where(r => r.Tip == 1))
                            {
                                <div class="lightbox">
                                    <a href="~/Upload/slide/@item.Resim">
                                        <span class="thumb-info thumb-info-lighten">
                                            <span class="thumb-info-wrapper">
                                                <img alt="@item.Aciklama" title="@item.Aciklama" class="img-responsive img-thumbnail" src="~/Upload/slide/@item.Resim" style="width:100%;height:400px;">
                                            </span>
                                        </span>
                                    </a>
                                </div>
                            }
                        </div>
                    </div>
                </div>
                <div class="panel panel-accordion panel-accordion-success">
                    <div class="panel-heading" style="background-color:aliceblue;color:blue;font-weight:bold;">
                        <h4  style="background-color:aliceblue;color:blue;font-weight:bold;">
                            <a  class="accordion-toggle" style="background-color:aliceblue;color:blue;font-weight:bold;"  data-toggle="collapse" data-parent="#accordionSuccess" href="#collapseSuccessDetaylar">
                                Detaylar
                            </a>
                        </h4>
                    </div>
                    <div id="collapseSuccessDetaylar" class="accordion-body collapse">
                        <div class="panel-body">
                            <div>
                                <dl>
                                    @foreach (var ModelDetaylar in db.Kurumlar.Where(h => h.Sponsorlar == "Ayın En İyi Anaokulu").Take(1).ToList())
                                    {
                                        if (ModelDetaylar.Adres != null)
                                        {
                                            <dt style="background-color:#FFF;width:95%;"><span style="color:red;font-weight:bold;margin-left:10px;">ADRESİ :</span></dt>
                                                <dd style="width:95%;background-color:#FFF;"><span style="font-weight:bold;margin-left:10px;">@ModelDetaylar.Adres</span></dd>
                                        }

                                        if (ModelDetaylar.Tel != null)
                                        {
                                            <dt style="background-color:#FFF;width:95%;"><span style="color:red;font-weight:bold;margin-left:10px;">TELEFON NO :</span></dt>
                                                <dd style="width:95%;background-color:#FFF;"><span style="font-weight:bold;margin-left:10px;">@ModelDetaylar.Tel</span></dd>
                                        }
                                        if (ModelDetaylar.Subeler != null)
                                        {
                                            <dt style="background-color:#FFF;width:95%;"><span style="color:red;font-weight:bold;margin-left:10px;">ŞUBELER :</span></dt>
                                                <dd style="width:95%;background-color:#FFF;"><span style="font-weight:bold;margin-left:10px;">@ModelDetaylar.Subeler</span></dd>
                                        }
                                        if (ModelDetaylar.KurulusTarih != null)
                                        {
                                            <dt style="background-color:#FFF;width:95%;"><span style="color:red;font-weight:bold;margin-left:10px;">AÇILIŞ YILI :</span></dt>
                                                <dd style="width:95%;background-color:#FFF;"><span style="font-weight:bold;margin-left:10px;">@ModelDetaylar.KurulusTarih</span></dd>
                                        }
                                        if (ModelDetaylar.Kurucusu != null)
                                        {
                                            <dt style="background-color:#FFF;width:95%;"><span style="color:red;font-weight:bold;margin-left:10px;">KURUCUSU \ YÖNETİCİSİ :</span></dt>
                                                <dd style="width:95%;background-color:#FFF;"><span style="font-weight:bold;margin-left:10px;">@ModelDetaylar.Kurucusu</span></dd>
                                        }
                                        if (ModelDetaylar.Kordinatorler != null)
                                        {
                                            <dt style="background-color:#FFF;width:95%;"><span style="color:red;font-weight:bold;margin-left:10px;">KORDİNATÖRLER :</span></dt>
                                                <dd style="width:95%;background-color:#FFF;"><span style="font-weight:bold;margin-left:10px;">@ModelDetaylar.Kordinatorler</span></dd>
                                        }
                                        if (ModelDetaylar.KurumMuduru != null)
                                        {
                                            <dt style="background-color:#FFF;width:95%;"><span style="color:red;font-weight:bold;margin-left:10px;">MÜDÜR :</span></dt>
                                                <dd style="width:95%;background-color:#FFF;"><span style="font-weight:bold;margin-left:10px;">@ModelDetaylar.KurumMuduru</span></dd>
                                        }
                                        if (ModelDetaylar.KurumMuduru != null)
                                        {
                                            <dt style="background-color:#FFF;width:95%;"><span style="color:red;font-weight:bold;margin-left:10px;">MÜDÜR YARDIMCILARI :</span></dt>
                                                <dd style="width:95%;background-color:#FFF;"><span style="font-weight:bold;margin-left:10px;">@ModelDetaylar.KurumMuduru</span></dd>
                                        }
                                        if (ModelDetaylar.Egitimciler != null)
                                        {
                                            <dt style="background-color:#FFF;width:95%;"><span style="color:red;font-weight:bold;margin-left:10px;">EĞİTİMCİLER :</span></dt>
                                                <dd style="width:95%;background-color:#FFF;"><span style="font-weight:bold;margin-left:10px;">@ModelDetaylar.Egitimciler</span></dd>
                                        }
                                        if (ModelDetaylar.Aktiviteler != null)
                                        {
                                            <dt style="background-color:#FFF;width:95%;"><span style="color:red;font-weight:bold;margin-left:10px;">AKTİVİTELER :</span></dt>
                                                <dd style="width:95%;background-color:#FFF;"><span style="font-weight:bold;margin-left:10px;">@ModelDetaylar.Aktiviteler</span></dd>
                                        }
                                        if (ModelDetaylar.Fax != null)
                                        {
                                            <dt style="background-color:#FFF;width:95%;"><span style="color:red;font-weight:bold;margin-left:10px;">FAKS :</span></dt>
                                                <dd style="width:95%;background-color:#FFF;"><span style="font-weight:bold;margin-left:10px;">@ModelDetaylar.Fax</span></dd>
                                        }
                                        if (ModelDetaylar.Email != null)
                                        {
                                            <dt style="background-color:#FFF;width:95%;"><span style="color:red;font-weight:bold;margin-left:10px;">E-MAİL :</span></dt>
                                                <dd style="width:95%;background-color:#FFF;"><span style="font-weight:bold;margin-left:10px;">@ModelDetaylar.Email</span></dd>
                                        }

                                        if (ModelDetaylar.EgitimDili != null)
                                        {
                                            <dt style="background-color:#FFF;width:95%;"><span style="color:red;font-weight:bold;margin-left:10px;">EĞİTİM DİLİ :</span></dt>
                                                <dd style="width:95%;background-color:#FFF;"><span style="font-weight:bold;margin-left:10px;">@ModelDetaylar.EgitimDili</span></dd>
                                        }
                                        if (ModelDetaylar.OgrenciSayisi != null)
                                        {
                                            <dt style="background-color:#FFF;width:95%;"><span style="color:red;font-weight:bold;margin-left:10px;">ÖĞRENCİ SAYISI :</span></dt>
                                                <dd style="width:95%;background-color:#FFF;"><span style="font-weight:bold;margin-left:10px;">@ModelDetaylar.OgrenciSayisi</span></dd>
                                        }
                                        if (ModelDetaylar.YurticiUniversiteBasariOrani != null)
                                        {
                                            <dt style="background-color:#FFF;width:95%;"><span style="color:red;font-weight:bold;margin-left:10px;">YURT İÇİ ÜNİVERSİTE BAŞARI ORANI :</span></dt>
                                                <dd style="width:95%;background-color:#FFF;"><span style="font-weight:bold;margin-left:10px;">@ModelDetaylar.YurticiUniversiteBasariOrani</span></dd>
                                        }
                                        if (ModelDetaylar.YurtDisiUniversiteBasariOrani != null)
                                        {
                                            <dt style="background-color:#FFF;width:95%;"><span style="color:red;font-weight:bold;margin-left:10px;">YURT DIŞI ÜNİVERSİTE BAŞARI ORANI :</span></dt>
                                                <dd style="width:95%;background-color:#FFF;"><span style="font-weight:bold;margin-left:10px;">@ModelDetaylar.YurtDisiUniversiteBasariOrani</span></dd>
                                        }
                                        if (ModelDetaylar.BursOlanakları != null)
                                        {
                                            <dt style="background-color:#FFF;width:95%;"><span style="color:red;font-weight:bold;margin-left:10px;">BURS OLANAKLARI :</span></dt>
                                                <dd style="width:95%;background-color:#FFF;"><span style="font-weight:bold;margin-left:10px;">@ModelDetaylar.BursOlanakları</span></dd>
                                        }
                                        if (ModelDetaylar.Ucreti != null)
                                        {
                                            <dt style="background-color:#FFF;width:95%;"><span style="color:red;font-weight:bold;margin-left:10px;">ÜCRETİ :</span></dt>
                                                <dd style="width:95%;background-color:#FFF;"><span style="font-weight:bold;margin-left:10px;">@ModelDetaylar.Ucreti</span></dd>
                                        }
                                    }



                                </dl>
                            </div>

                        </div>
                    </div>
                </div>

                <div class="panel panel-accordion panel-accordion-success">
                    <div class="panel-heading" style="background-color:aliceblue;color:blue;font-weight:bold;">
                        <h4  style="background-color:aliceblue;color:blue;font-weight:bold;">
                            <a  class="accordion-toggle" style="background-color:aliceblue;color:blue;font-weight:bold;"  data-toggle="collapse" data-parent="#accordionSuccess" href="#collapseSuccessMesaj">
                                Mesaj Gönderin
                            </a>
                        </h4>
                    </div>
                    <div id="collapseSuccessMesaj" class="accordion-body collapse">
                        <div class="panel-body">
                            @Html.Partial("/Views/_Partial/_kurumMesajGonder.cshtml", Model)
                        </div>
                    </div>
                </div>       
                
                
            </div>

            
                        }

    }

 